{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-sendfile-poc"
		},
		"Azure Function Send File_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'Azure Function Send File'"
		},
		"Azure Function Send File_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://adf-send-file.azurewebsites.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-sendfile-poc_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Azure Function Send File_functionKey": {
						"value": "[parameters('Azure Function Send File_functionKey')]"
					},
					"Azure Function Send File_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('Azure Function Send File_properties_typeProperties_functionAppUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}